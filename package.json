{
  "name": "@erinmyoung/erinmyoung-ui",
  "private": false,
  "version": "1.0.0",
  "description": "A modern React component library built with Storybook, TypeScript, styled-components, and Framer Motion",
  "author": "Erin Young erinmyoung12@gmail.com",
  "license": "MIT",
  "keywords": [
    "react",
    "typescript",
    "components",
    "storybook",
    "ui",
    "styled-components",
    "framer-motion",
    "theming"
  ],
  "homepage": "https://github.com/erinmyoung/erinmyoung-ui#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/erinmyoung/erinmyoung-ui.git"
  },
  "bugs": {
    "url": "https://github.com/erinmyoung/erinmyoung-ui/issues"
  },
  "type": "module",
  "main": "dist/index.cjs.js",
  "module": "dist/index.es.js",
  "types": "dist/index.d.ts",
  "files": [
    "/dist"
  ],
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "lint": "eslint src --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint src --fix",
    "preview": "vite preview",
    "format": "prettier --write --parser typescript '**/*.{ts,tsx}'",
    "test": "vitest run",
    "test-watch": "vitest",
    "test:ui": "vitest --ui",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "prepare": "husky && npm run build",
    "precommit": "npm run lint && npm test"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/@erinmyoung"
  },
  "dependencies": {
    "framer-motion": "^12.23.12",
    "react": "^19.1.0",
    "react-dom": "^19.1.0"
  },
  "peerDependencies": {
    "react": "^19.1.0",
    "react-dom": "^19.1.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.30.1",
    "@storybook/addon-a11y": "^9.1.2",
    "@storybook/addon-docs": "^9.1.0",
    "@storybook/react-vite": "^9.1.0",
    "@testing-library/jest-dom": "^6.6.4",
    "@testing-library/react": "^16.3.0",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "@types/styled-components": "^5.1.34",
    "@vitejs/plugin-react": "^4.6.0",
    "@vitest/ui": "^3.2.4",
    "eslint": "^9.30.1",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "eslint-plugin-storybook": "^9.1.0",
    "globals": "^16.3.0",
    "husky": "^9.1.7",
    "jsdom": "^26.1.0",
    "prettier": "^3.6.2",
    "storybook": "^9.1.0",
    "styled-components": "^6.1.19",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.35.1",
    "vite": "^7.0.4",
    "vite-plugin-dts": "^4.5.4",
    "vitest": "^3.2.4"
  }
}
